@model nmct.ba.cashlessproject.web.PresentationModels.PMRegisterLog
<section class="table">
    <ul class="header">
        <li>
            @Html.DisplayNameFor(model => model.RegisterLogNew.RegisterID)
        </li>
        <li>
            @Html.DisplayNameFor(model => model.RegisterLogNew.RegisterName)
        </li>
        <li>
            @Html.DisplayNameFor(model => model.RegisterLogNew.RegisterPurchaseDate)
        </li>
        <li>
            @Html.DisplayNameFor(model => model.RegisterLogNew.LogMessage)
        </li>
        <li>
            @Html.DisplayNameFor(model => model.RegisterLogNew.LogStackTrace)
        </li>
        <li>
            @Html.DisplayNameFor(model => model.RegisterLogNew.LogTimestamp)
        </li>

    </ul>
    @for (int i = 0; i < Model.RegisterLogs.Count; i++)
    {
        DateTime purchaseDate = nmct.ba.cashlessproject.web.Helper.TimeConverter.ToDateTime(Model.RegisterLogs[i].RegisterPurchaseDate);
        DateTime logTimestamp = nmct.ba.cashlessproject.web.Helper.TimeConverter.ToDateTime(Model.RegisterLogs[i].LogTimestamp);

        <ul class="data">
            <li>@Model.RegisterLogs[i].RegisterID</li>
            <li>@Model.RegisterLogs[i].RegisterName</li>
            <li>@purchaseDate.ToString("MM-dd-yyyy")</li>
            <li>@Model.RegisterLogs[i].LogMessage</li>
            <li>@Model.RegisterLogs[i].LogStackTrace</li>
            <li>@logTimestamp.ToString("MM-dd-yyyy hh:mm:ss")</li>
        </ul>
    }
</section>